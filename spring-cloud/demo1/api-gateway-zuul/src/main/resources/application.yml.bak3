spring:
  application:
    name: api-gateway-zuul
server:
  port: 10401
eureka:
  client:
    service-url:
      defaultZone: http://localhost:10001/eureka
  instance:
    prefer-ip-address: true     # 在应用中显示 IP
spring.security:
  user:
    name: user
    password: 123
  basic:
    enabled: true
zuul:
  routes:
    myid1:                      # 全局唯一即可，自己指定
      path: /abcd/**            # 映射路径
      serviceId: user-provider  # 映射服务
